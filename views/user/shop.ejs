          <div class="products" style="padding: 120px;">
            <div class="row">
              <% for(let i = 0; i < products.length; i++) { %>
              <div class="col-6 col-md-4 col-lg-4 col-xl-3">
                <div class="product">
                  <figure class="product-media">
                    <a href="/product?id=<%= products[i]._id %>">
                      <img src="../../uploads/<%= products[i].images[0]%>" alt="Product image" class="product-image">
                    </a>
                    <div class="product-action-vertical">
                      <a href="#" class="btn-product-icon btn-wishlist btn-expandable <%= products[i].isInWishlist ? 'in-wishlist' : '' %>" data-id="<%= products[i]._id %>">
                        <span><%= products[i].isInWishlist ? 'Remove from wishlist' : 'Add to wishlist' %></span>
                    </a>             </div><!-- End .product-action -->
                    <div class="product-action action-icon-top">
                      <button class="btn-product btn-cart" data-id="<%= products[i]._id %>"><span>add to cart</span></button>
                    </div><!-- End .product-action -->
                  </figure><!-- End .product-media -->
                  <div class="product-body">
                    <div class="product-cat">
                      <a href="#"><% products[i].category %></a>
                    </div><!-- End .product-cat -->
                    <h3 class="product-title"><a href="/product?id=<%= products[i]._id %>"><%= products[i].name %></a></h3><!-- End .product-title -->
                    <div class="product-price">
                      ₹<%= products[i].price %>
                    </div><!-- End .product-price -->
                    <!-- <div>
											<p><%= products[i].description %></p>
										</div> -->
                  </div><!-- End .product-body -->
                </div><!-- End .product -->
              </div><!-- End .col-sm-6 col-lg-4 col-xl-3 -->
              <% } %>
            </div><!-- End .row -->

