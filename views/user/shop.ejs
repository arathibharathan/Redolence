          <div class="load-more-container text-center">
            <form action="/shop" method="GET" style="display:inline;">
                <input type="hidden" name="page" value="<%= prevPage %>">
                <input type="hidden" name="sort" value="<%= sort %>">
                <button style="background-color: #c96;" <% if (prevPage < 1) { %> disabled <% } %> >
                    Prev
                </button>
            </form>
        
            <% for (let i = 0; i < pages; i++) { %>
                <form action="/shop" method="GET" style="display:inline;">
                    <input type="hidden" name="page" value="<%= i + 1 %>">
                    <input type="hidden" name="sort" value="<%= sort %>">
                    <button style="width: 30px; height: 31px; background-color: #c96" type="submit" <% if (i + 1 === page) { %> disabled <% } %> >
                        <%= i + 1 %>
                    </button>
                </form>
            <% } %>
        
            <form action="/shop" method="GET" style="display:inline;">
                <input type="hidden" name="page" value="<%= nextPage %>">
                <input type="hidden" name="sort" value="<%= sort %>">
                <button style="background-color: #c96;" <% if (nextPage > pages) { %> disabled <% } %> >
                    Next
                </button>
            </form>
        </div>
        


