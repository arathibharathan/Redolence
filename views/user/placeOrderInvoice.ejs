      <div class="invoice-container">
        <div class="invoice-header">
            <div class="company-info">
              <img src="/Redolence logo-01.png" alt="Redolence Logo" style="width:315px; height: 126px;">
                <h2>Redolence Store</h2>
                <p>123 E-Commerce Street, Shopping District</p>
                <p>Contact: +0123 456 789 | support@redolence.com</p>
            </div>
            
            <div class="invoice-details">
                <h1>Order Invoice</h1>
                <div class="invoice-meta">
                    <div class="meta-item">
                        <strong>Order Number:</strong>
                        <span data-order-id="<%= orders.order_id %>">#<%= orders.order_id %></span>
                    </div>
                    <div class="meta-item">
                        <strong>Order Date:</strong>
                        <span><%= new Date(orders.orderedAt).toLocaleString() %></span>
                    </div>
                    <div class="meta-item">
                        <strong>Order Status:</strong>
                        <span class="order-status 
                            <%= 
                                orders.orderStatus === 'Placed' ? 'status-placed' : 
                                orders.orderStatus === 'Shipped' ? 'status-shipped' : 
                                orders.orderStatus === 'Delivered' ? 'status-delivered' : 
                                'status-cancelled'
                            %>
                        ">
                            <%= orders.orderStatus %>
                        </span>
                    </div>
                </div>
            </div>
        </div>
    
        <div class="invoice-body">
            <div class="order-summary">
                <h3>Order Summary</h3>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>Product Name</th>
                            <th>Quantity</th>
                            <th>Unit Price</th>
                            <th>Total Price</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% orders.orderItems.forEach(item => { %>
                            <tr>
                                <td><%= item.name %></td>
                                <td class="text-center"><%= item.quantity %></td>
                                <td class="text-right">₹<%= item.price.toFixed(2) %></td>
                                <td class="text-right">₹<%= item.totalPrice.toFixed(2) %></td>
                            </tr>
                        <% }) %>
                    </tbody>
                    <tfoot>
                        <tr>
                            <th colspan="3" class="text-right">Total Amount:</th>
                            <th class="text-right">₹<%= orders.totalAmount.toFixed(2) %></th>
                        </tr>
                    </tfoot>
                </table>
            </div>
    
            <div class="shipping-info">
                <h3>Shipping Information</h3>
                <% if (orders.address && orders.address.length > 0) { %>
                    <% orders.address.forEach((address, index) => { %>
                        <div class="address-card">
                            <p><strong>Name:</strong> <%= address.name %></p>
                            <p><strong>Mobile:</strong> <%= address.mobile %></p>
                            <p><strong>Address:</strong> 
                                <%= address.streetAddress %>, 
                                <%= address.landmark ? address.landmark + ', ' : '' %>
                                <%= address.city %>, 
                                <%= address.state %> - 
                                <%= address.pinCode %>
                            </p>
                        </div>
                    <% }) %>
                <% } else { %>
                    <p>No shipping address found.</p>
                <% } %>
            </div>
    
            <div class="payment-info">
                <h3>Payment Details</h3>
                <p><strong>Payment Method:</strong> <%= orders.paymentMethod %></p>
                <p><strong>Total Paid:</strong> ₹<%= orders.totalAmount.toFixed(2) %></p>
            </div>
        </div>
    
        <div class="invoice-footer">
            <p>Thank you for shopping with Redolence Store!</p>
            <p>For any queries, contact our customer support.</p>
        </div>
    
        <div class="invoice-actions">
          <button onclick="window.print()" class="btn btn-primary print-btn">
              <i class="icon-print"></i> Print Invoice
          </button>
          <button onclick="downloadPDF()" class="btn btn-success download-btn" style="margin-left: 10px;">
              <i class="icon-download"></i> Download PDF
          </button>
      </div>
