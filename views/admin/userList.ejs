    <script>


function block(id){
 
 axios.post('/admin/blockUser',{id:id})  //or fetch
}                    



       function block(id) {
    fetch('/admin/blockUser', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({ id: id }),
    })
    .then(response => {
        if (!response.ok) {
            throw new Error('Network response was not ok');
        }
        return response.json();
    })
    .then(data => {
        console.log('User blocked successfully:', data);
        window.location.reload()
    })
    .catch(error => {
        console.error('Error blocking user:', error);
    });
}



// function unblock(id){
//           axios.post('/admin/unblockUser',{id:id})
//           console.log("unblocked");
          
//         }   




function unblock(id) {
    fetch('/admin/unblockUser', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({ id: id }),
    })
    .then(response => {
        if (!response.ok) {
            throw new Error('Network response was not ok');
        }
        return response.json();
    })
    .then(data => {
        console.log("unblocked", data);
        window.location.reload()

    })
    .catch(error => {
        console.error('Error unblocking user:', error);
    });
}

